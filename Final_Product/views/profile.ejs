                  <div class="user-profile">
                    <div class="row">
                      <div class="col-lg-4">
                        <div class="user-photo m-b-30">
                          <img class="img-fluid" src="assets/images/profile.jpg" alt="" />
                        </div>


                      </div>
                      <div class="col-lg-8">
                        <form action="/profile" method="POST">
                        <div class="user-profile-name">Jashandeep Singh</div>
                        <div class="user-Location">
                          <i class="ti-location-pin"></i><input type="text" placeholder="Patiala, Punjab" />
                        </div>
                        <div class="user-job-title">SDE</div>
                        <div class="ratings">
                          <h4>Ratings</h4>
                          <div class="rating-star">
                            <span>8.9</span>
                            <i class="ti-star color-primary"></i>
                            <i class="ti-star color-primary"></i>
                            <i class="ti-star color-primary"></i>
                            <i class="ti-star color-primary"></i>
                            <i class="ti-star"></i>
                          </div>
                        </div>
                        <div class="custom-tab user-profile-tab">
                          <ul class="nav nav-tabs" role="tablist">
                            <li role="presentation" class="active">
                              <a href="#1" aria-controls="1" role="tab" data-toggle="tab">About</a>
                            </li>
                          </ul>
                          <div class="tab-content">
                            <div role="tabpanel" class="tab-pane active" id="1">
                              <div class="contact-information">
                                <h4>Contact information</h4>
                                <div class="phone-content">
                                  <span class="contact-title">Phone:</span>
                                  <span class="phone-number"><input type="text" placeholder="+91 00000000000" name="phone"/></span>
                                </div>
                                <div class="address-content">
                                  <span class="contact-title">Address:</span>
                                  <span class="mail-address"><input type="text" placeholder="XXXXXXXXXXXXXXXX" name="address"/></span>
                                </div>
                                <div class="email-content">
                                  <span class="contact-title">Email:</span>
                                  <span class="contact-email">abcd@gmail.com</span>
                                </div>
                                <div class="basic-information">
                                  <h4>Basic information</h4>
                                  <div class="birthday-content">
                                    <span class="contact-title">Birthday:</span>
                                    <span class="birth-date"><input type="text" placeholder="Nov 16, 2000" name="birthday"/> </span>
                                  </div>
                                  <div class="gender-content">
                                    <span class="contact-title">Gender:</span>
                                    <span class="gender"><input type="text" placeholder="Male" name="gender"/>
                                    </span>
                                  </div>

                                  <h4>Departments</h4>
                                  <div class="form-check form-check-inline">
                                    <input class="form-check-input" type="radio" name="inlineRadioOptions" id="inlineRadio1" value="Mechanical" onclick="addMech()">
                                    <label class="form-check-label" for="inlineRadio1">Mechanical</label>
                                  </div>
                                  <div class="form-check form-check-inline">
                                    <input class="form-check-input" type="radio" name="inlineRadioOptions" id="inlineRadio2" value="Electrical" onclick="addElec()">
                                    <label class="form-check-label" for="inlineRadio2">Electrical</label>
                                  </div>
                                  <div class="form-check form-check-inline">
                                    <input class="form-check-input" type="radio" name="inlineRadioOptions" id="inlineRadio2" value="IT" onclick="addIT()">
                                    <label class="form-check-label" for="inlineRadio2">Computer Science</label>
                                  </div>

                                  <h4>Skills</h4>
                                  <div id="mech">
                                    <div class="form-check form-check-inline">
                                      <input class="form-check-input" type="checkbox" name="skills1" id="inlineCheckbox1" value="robotics">
                                      <label class="form-check-label" for="inlineCheckbox1">Robotics</label>
                                    </div>
                                    <div class="form-check form-check-inline">
                                      <input class="form-check-input" type="checkbox" name="skills2" id="inlineCheckbox2" value="NanoTechnology">
                                      <label class="form-check-label" for="inlineCheckbox2">Nano Technology</label>
                                    </div>
                                    <div class="form-check form-check-inline">
                                      <input class="form-check-input" type="checkbox" name="skills3" id="inlineCheckbox3" value="GeoMech">
                                      <label class="form-check-label" for="inlineCheckbox2">Geo Mechanics</label>
                                    </div>
                                  </div>

                                  
                                  <div id="elec">
                                    <div class="form-check form-check-inline">
                                      <input class="form-check-input" type="checkbox" name="skills1" id="inlineCheckbox1" value="autocad">
                                      <label class="form-check-label" for="inlineCheckbox1">Autocad</label>
                                    </div>
                                    <div class="form-check form-check-inline">
                                      <input class="form-check-input" type="checkbox" name="skills2" id="inlineCheckbox2" value="matlab">
                                      <label class="form-check-label" for="inlineCheckbox2">Matlab</label>
                                    </div>
                                    <div class="form-check form-check-inline">
                                      <input class="form-check-input" type="checkbox" name="skills3" id="inlineCheckbox3" value="digElec">
                                      <label class="form-check-label" for="inlineCheckbox2">Digital Electronics</label>
                                    </div>
                                  </div>


                                  <div id="infor">
                                    <div class="form-check form-check-inline">
                                      <input class="form-check-input" type="checkbox" name="skills1" id="inlineCheckbox1" value="C">
                                      <label class="form-check-label" for="inlineCheckbox1">C</label>
                                    </div>
                                    <div class="form-check form-check-inline">
                                      <input class="form-check-input" type="checkbox" name="skills2" id="inlineCheckbox2" value="c++">
                                      <label class="form-check-label" for="inlineCheckbox2">C++</label>
                                    </div>
                                    <div class="form-check form-check-inline">
                                      <input class="form-check-input" type="checkbox" name="skills3" id="inlineCheckbox3" name="skills3" value="java">
                                      <label class="form-check-label" for="inlineCheckbox2">Java</label>
                                    </div>
                                  </div>
                                </div>
                              </div>
                            </div>
