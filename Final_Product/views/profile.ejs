  <script>
    document.querySelector('#mech').style.visibility = "hidden";
    document.querySelector('#elec').style.visibility = "hidden";
    document.querySelector('#infor').style.visibility = "hidden";
    function addMech(){
      document.querySelector('#mech').style.visibility = "visible";
      document.querySelector('#elec').style.visibility = "hidden";
      document.querySelector('#infor').style.visibility = "hidden";
    }
    function addElec(){
      document.querySelector('#elec').style.visibility = "visible";
      document.querySelector('#mech').style.visibility = "hidden";
      document.querySelector('#infor').style.visibility = "hidden";
    }
    function addIT(){
      document.querySelector('#infor').style.visibility = "visible";
      document.querySelector('#mech').style.visibility = "hidden";
    document.querySelector('#elec').style.visibility = "hidden";
    }
  </script>

